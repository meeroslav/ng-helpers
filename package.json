{
  "name": "ng-helpers",
  "version": "0.1.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run copy:readme",
    "test": "ng test",
    "test:lib-coverage": "ng test ng-helpers --code-coverage",
    "report-coverage:lib": "cat ./coverage/ng-helpers/lcov.info | codecov",
    "copy:readme": "copyfiles ./README.md ./dist/ng-helpers",
    "lint": "ng lint",
    "lint:lib": "ng lint ng-helpers"
  },
  "private": true,
  "dependencies": {
    "@angular/core": "~8.2.14",
    "@ngrx/effects": "~8.6.0",
    "@ngrx/store": "~8.6.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.2",
    "@angular-devkit/build-ng-packagr": "~0.803.2",
    "@angular/cli": "~8.3.21",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/platform-browser": "^8.2.4",
    "@angular/platform-browser-dynamic": "^8.2.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codecov": "^3.6.1",
    "codelyzer": "^5.0.0",
    "copyfiles": "^2.1.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.4.0",
    "serialize-javascript": ">=2.1.1",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "zone.js": "~0.9.1"
  }
}
