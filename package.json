{
  "name": "ng-helpers",
  "version": "0.7.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod && npm run copy:readme",
    "test": "ng test",
    "test:lib-coverage": "ng test ng-helpers --code-coverage",
    "report-coverage:lib": "cat ./coverage/ng-helpers/lcov.info | codecov",
    "copy:readme": "copyfiles ./README.md ./dist/ng-helpers",
    "lint": "ng lint",
    "lint:lib": "ng lint ng-helpers"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meeroslav/ng-helpers.git"
  },
  "keywords": [
    "angular",
    "helpers",
    "media",
    "fragment"
  ],
  "author": "Miroslav Jonas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meeroslav/ng-helpers/issues"
  },
  "homepage": "https://github.com/meeroslav/ng-helpers",
  "dependencies": {
    "@angular/core": "~10.0.14",
    "rxjs": "^6.5.3",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.8",
    "@angular-devkit/build-ng-packagr": "~0.1000.8",
    "@angular/cli": "^10.0.8",
    "@angular/common": "^10.0.14",
    "@angular/compiler": "~10.0.14",
    "@angular/compiler-cli": "~10.0.14",
    "@angular/platform-browser": "^10.0.14",
    "@angular/platform-browser-dynamic": "^10.0.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codecov": "^3.6.1",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.1.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "serialize-javascript": "^4.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7",
    "zone.js": "~0.10.2"
  }
}
