{
  "name": "ng-helpers",
  "version": "0.1.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run copy:readme",
    "test": "ng test",
    "test:lib-coverage": "ng test ng-helpers --code-coverage",
    "report-coverage:lib": "cat ./coverage/ng-helpers/lcov.info | codecov",
    "copy:readme": "copyfiles ./README.md ./dist/ng-helpers",
    "lint": "ng lint",
    "lint:lib": "ng lint ng-helpers"
  },
  "private": true,
  "dependencies": {
    "@angular/core": "~9.1.9",
    "@ngrx/effects": "~9.2.0",
    "@ngrx/store": "~9.2.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular-devkit/build-ng-packagr": "~0.901.7",
    "@angular/cli": "^9.1.7",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/compiler-cli": "~9.1.9",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codecov": "^3.6.5",
    "codelyzer": "^5.1.2",
    "copyfiles": "^2.1.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "ng-packagr": "^9.0.0",
    "serialize-javascript": ">=2.1.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3",
    "zone.js": "~0.10.2"
  }
}
